{
  "name": "@openjung/core",
  "version": "1.0.1",
  "description": "Core OEJTS (Open Extended Jungian Type Scales) MBTI personality test questions and scoring logic",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mbti",
    "oejts",
    "openjung",
    "personality",
    "psychology",
    "test",
    "jung",
    "jungian"
  ],
  "author": "ya-luotao <luotao@hey.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openjung/core.git"
  },
  "bugs": {
    "url": "https://github.com/openjung/core/issues"
  },
  "homepage": "https://openjung.org",
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^4.0.16"
  }
}
